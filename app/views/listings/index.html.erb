<p id="notice"><%= notice %></p>

<h1>Listing Listings</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Depart maps</th>
      <th>Depart loc</th>
      <th>Depart range start</th>
      <th>Depart range end</th>
      <th>Dest maps</th>
      <th>Dest loc</th>
      <th>Dest range start</th>
      <th>Dest range end</th>
      <th>Type</th>
      <th>Money</th>
      <th>Comments</th>
      <th>Detour time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.user_id %></td>
        <td><%= listing.depart_maps_id %></td>
        <td><%= listing.depart_loc_id %></td>
        <td><%= listing.depart_range_start %></td>
        <td><%= listing.depart_range_end %></td>
        <td><%= listing.dest_maps_id %></td>
        <td><%= listing.dest_loc_id %></td>
        <td><%= listing.dest_range_start %></td>
        <td><%= listing.dest_range_end %></td>
        <td><%= listing.type %></td>
        <td><%= listing.money %></td>
        <td><%= listing.comments %></td>
        <td><%= listing.detour_time %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
